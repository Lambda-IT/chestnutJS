{
    "name": "chestnutjs",
    "version": "1.0.4",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start:client": "cd ./client && npm start",
        "start:server": "cd ./server && npm start",
        "start": "npm run start:server",
        "build:server": "cd ./server && npm run build",
        "build:client": "cd ./client && npm run build",
        "build": "npm run build:server && npm run build:client",
        "build:prod": "tsc -p . --outDir . --declaration && npm run build:client",
        "postinstall": "cd ./server && npm install && cd .. && tsc -p . --outDir . --declaration && cd ./client && npm install && npm run build:client"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "concurrently": "^3.6.0",
        "prettier": "^1.13.7",
        "typescript": "2.7.2"
    }
}
